<?xml version="1.0"?>

<launch>

   <node name="rplidarNode"  pkg="rplidar_ros"  type="rplidarNode" output="screen" respawn="false" >
    <param name="port"         type="string" value="/dev/ttyUSB0"/>  
    <param name="baudrate"     type="int"    value="256000"/>
<!--    <param name="frame_id"     type="string" value="laser_frame"/>-->
    <param name="frame_id"     type="string" value="base_scan"/>
    <param name="low_exposure"  type="bool"   value="false"/>
    <param name="resolution_fixed"    type="bool"   value="true"/>
    <param name="auto_reconnect"    type="bool"   value="true"/>
    <param name="reversion"    type="bool"   value="false"/>
    <param name="angle_min"    type="double" value="-60" />
    <param name="angle_max"    type="double" value="60" />
    <param name="range_min"    type="double" value="0.1" />
    <param name="range_max"    type="double" value="16.0" />
    <param name="ignore_array" type="string" value="" />
    <param name="samp_rate"    type="int"    value="9"/>
    <param name="frequency"    type="double" value="7"/>
  </node>

</launch>

